{
    "plugin": "default",
    "zones": 20,
    "zone_map": {
        "1": "<mac>"
    },
    "lms_host": "localhost",
    "lms_port": 9000,
    "lms_cli_port": 9090,
    "uploadPath": "/Volumes/Music_Files",
    "uploadPlaybackPath": "/home/music/music_files",
    "cors_port": 7092,
    "macAddress": "",
    "useSlowQueueWorkaround": true,
    "scripts": {
        "playUploadedFile": "echo {{name}} {{zones}} {{path}}",
