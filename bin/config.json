{
    "plugin": "default",
    "zones": 20,
    "zone_map": {
        "1": "<mac>"
    },
    "lms_host": "localhost",
    "lms_port": 9000,
    "lms_cli_port": 9090,
    "msApi": "LWSS V 1.4.10.06 | ~API:1.6~",
    "ms": {
        "host": "<host>",
        "users": [
            { "user": "<user1>", "password": "<pw1>" }
        ]
    },
    "uploadPath": ".",
    "uploadPlaybackPath": "/home/music/music_files",
    "cors_port": 7092,
    "macAddress": "",
