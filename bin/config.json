    "sayFav": true,
    "filteredRadioEntries": [
        "search",
        "presets",
        "picks",
        "language",
        "talk",
        "sports"
    ],
    "filteredSpotifyEntries": [
    ],
    "scripts": {
        "playUploadedFile": "echo {{name}} {{zones}} {{path}}",
        "playAlarmSound": "echo {{name}} {{type}} {{volume}}",
        "playTTS": "echo {{name}} {{language}} {{text}} {{volume}}",
        "changeEqualizer": "echo {{name}} {{zones}}",
        "changeDefaultVolume": "echo {{name}} {{zones}} {{defaultVolume}}",
        "changeMaxVolume": "echo {{name}} {{zones}} {{maxVolume}}",
        "addNetworkShare": "echo {{name}} {{server}} {{guest}} {{username}} {{password}} {{configBase64}}",
        "sayFav": "echo {{name}} {{fav_id}} {{fav_name}} {{volume}}"
    }
